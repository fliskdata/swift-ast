{
  "name": "@flisk/swift-ast",
  "version": "0.1.0",
  "type": "module",
  "description": "Swift AST parsing in Node via WebAssembly (WASI), powered by SwiftSyntax + SwiftParser.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "wasm/swift_ast_wasi.wasm",
    "WASI.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build": "bash ./scripts/build-wasm.sh && npm run compile",
    "test": "node --test"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17"
  },
  "license": "MIT"
}
